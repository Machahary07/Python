   "source": [
    "# WAF that takes length and breadth of a rectangle & returns its perimeter and area\n",
    "def rect(l, b):\n",
    "    a = l * b\n",
    "    p = 2 * (l + b)\n",
    "    return a, p\n",
    "\n",
    "a, p = rect(2, 3)\n",
    "print(\"Area:\", a, \"Perimeter:\", p)"
   ]
